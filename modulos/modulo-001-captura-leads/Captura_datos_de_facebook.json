    {
      "parameters": {
        "jsCode": "for (const item of $input.all()) {\n  const nombre = item.json.nombre || '';\n  const telefonoRaw = item.json.telefono || '';\n  const estado = item.json.estado || '';\n\n  item.json.telefono_original = telefonoRaw;\n\n  // Inicializar campos de error\n  item.json.errores = [];\n\n  // Validar campos obligatorios\n  if (!nombre.trim()) item.json.errores.push(\"Nombre vac√≠o\");\n  if (!estado.trim()) item.json.errores.push(\"Estado vac√≠o\");\n  if (!telefonoRaw.trim()) item.json.errores.push(\"Tel√©fono vac√≠o\");\n\n  // Si hay errores, marcar como inv√°lido y saltar\n  if (item.json.errores.length > 0) {\n    item.json.telefono = \"INVALIDO\";\n    continue;\n  }\n\n  // Limpiar y normalizar tel√©fono\n  let telefono = telefonoRaw.replace(/\\D/g, '');\n\n  // Caso 1: N√∫mero ya normalizado (11 d√≠gitos con \"1\" al inicio)\n  if (telefono.startsWith('1') && telefono.length === 11) {\n    item.json.telefono = `+${telefono}`;\n  }\n  // Caso 2: N√∫mero de 10 d√≠gitos (sin \"1\")\n  else if (telefono.length === 10) {\n    item.json.telefono = `+1${telefono}`;\n  }\n  // Caso 3: Formato inv√°lido\n  else {\n    item.json.errores.push(`Formato inv√°lido: ${telefonoRaw}`);\n    item.json.telefono = \"INVALIDO\";\n  }\n}\n\nreturn $input.all();"
      },
